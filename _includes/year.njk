---
pagination:
  data: collections.posts
  size: 100
  alias: posts
  filter: post => post.date.getFullYear() === 2025
permalink: "/blog/2025/index.html"
---

{% extends "layout.njk" %}

{% block content %}
  <h2>Posts from 2025</h2>
  <ul>
    {% for post in posts %}
      <li><a href="{{ post.url }}">{{ post.data.title }}</a></li>
    {% endfor %}
  </ul>
{% endblock %}


